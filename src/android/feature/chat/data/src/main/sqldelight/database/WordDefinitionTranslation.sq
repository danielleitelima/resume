CREATE TABLE WordDefinitionTranslation (
    id TEXT PRIMARY KEY NOT NULL,
    content TEXT DEFAULT NULL,
    languagesCode TEXT NOT NULL,
    wordDefinitionId TEXT NOT NULL
);

insert:
INSERT OR REPLACE
INTO WordDefinitionTranslation
VALUES(?, ?, ?, ?);

getByWordDefinitionId:
SELECT *
FROM WordDefinitionTranslation
WHERE wordDefinitionId = :wordDefinitionId;

