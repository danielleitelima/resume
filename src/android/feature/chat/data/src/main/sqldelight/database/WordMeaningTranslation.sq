CREATE TABLE WordMeaningTranslation (
    id TEXT PRIMARY KEY NOT NULL,
    content TEXT DEFAULT NULL,
    languagesCode TEXT NOT NULL,
    wordMeaningId TEXT NOT NULL
);

insert:
INSERT OR REPLACE
INTO WordMeaningTranslation
VALUES(?, ?, ?, ?);

getByWordMeaningId:
SELECT *
FROM WordMeaningTranslation
WHERE wordMeaningId = :wordMeaningId;

